<div id="list-jobs">
  <div class="container mt-5">
    <h1>- Jobs -</h1>
    <hr/>
    <div class="row mt-5">
      <div class="col-sm-3"><b>Title</b></div>
      <div class="col-sm-3"><b>Location</b></div>
      <div class="col-sm-3"><b>Date</b></div>
      <div class="col-sm-3"><b>Description</b></div>
    </div>
    <% data.forEach(function(job)  { %>
    <div class="row mt-1">
      <div class="col-sm-3"><%= job.title %></div>
      <div class="col-sm-3"><%= job.location %></div>
      <div class="col-sm-3"><%= job.date %></div>
      <div class="col-sm-3"><%= job.description %></div>
    </div>
    <% }); %>
  </div>
</div>
<%- /* Expose locals as `window.SAILS_LOCALS` :: */ exposeLocalsToBrowser() %>
